< script type = "text/javascript" >
function sumas(num) {
if (num > 1) {
return num + sumas(num - 1);
} else {
return 1;
}
}
// Posible ver desde la Google Chrome >> Inspeccionar elemento >> Consola
console.log(sumas(9));
< /script>
< /body>
< /html>

