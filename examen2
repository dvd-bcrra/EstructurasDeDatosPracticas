<script type = "text/javascript" >
function dos_a_la_(ene) {
if (ene > 0) {
return 2 * dos_a_la_(ene - 1);
} else {
return 1;
}
}
// Posible ver desde la Google Chrome >> Inspeccionar elemento >> Consola
var ene = prompt("Ingrese la potencia", '');
console.log(dos_a_la_(ene));
< /script>
< /body>
